pipeline {
  agent any
  stages {
    stage('plan') {
      steps {
        echo 'to plan the devopment'
      }
    }

    stage('code') {
      steps {
        echo 'start working on the development'
      }
    }

    stage('build') {
      steps {
        echo 'build the code'
      }
    }

    stage('test') {
      steps {
        echo 'to test the code'
      }
    }

    stage('deploy') {
      steps {
        echo 'to deploy the app'
      }
    }

    stage('operate') {
      steps {
        echo 'to operate '
      }
    }

  }
}